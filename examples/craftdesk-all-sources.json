{
  "name": "full-example-project",
  "version": "1.0.0",
  "type": "skill",
  "description": "Example showing all dependency source types",

  "dependencies": {
    // Registry dependencies (default)
    "ruby-on-rails": "^7.0.0",
    "postgres-expert": "~1.2.0",

    // Registry with custom registry
    "@company/private-skill": {
      "version": "^3.0.0",
      "registry": "company-private"
    },

    // Direct registry URL
    "special-tool": {
      "version": "^2.0.0",
      "registry": "https://tools.example.com"
    },

    // Git dependencies with branch
    "experimental-agent": {
      "git": "https://github.com/user/agent-repo.git",
      "branch": "develop"
    },

    // Git with specific tag
    "stable-skill": {
      "git": "https://github.com/org/skills.git",
      "tag": "v2.1.0"
    },

    // Git with subdirectory (monorepo)
    "auth-handler": {
      "git": "https://github.com/company/monorepo.git",
      "branch": "main",
      "path": "packages/auth"
    },

    // Git with commit hash
    "specific-version": {
      "git": "https://github.com/user/repo.git",
      "commit": "a1b2c3d4e5f6789012345678901234567890abcd"
    },

    // Multiple skills from same git repo
    "data-processor": {
      "git": "https://github.com/company/monorepo.git",
      "tag": "v1.0.0",
      "path": "skills/data-processor"
    },
    "report-generator": {
      "git": "https://github.com/company/monorepo.git",
      "tag": "v1.0.0",
      "path": "skills/report-generator"
    }
  },

  "devDependencies": {
    "test-runner": "^1.0.0",
    "code-reviewer": {
      "git": "https://github.com/tools/reviewer.git",
      "branch": "latest"
    }
  },

  "registries": {
    "default": {
      "url": "https://craftdesk.ai"
    },
    "company-private": {
      "url": "https://registry.company.com",
      "scope": "@company"
    }
  }
}